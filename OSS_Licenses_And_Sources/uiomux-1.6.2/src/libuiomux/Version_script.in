#
# Export file for libuiomux
#
# Only the symbols listed in the global section will be callable from
# applications linking to libuiomux.
#

@PACKAGE@.so.0.0
{
        global:
		uiomux_open;
		uiomux_close;
		uiomux_lock;
		uiomux_unlock;
		uiomux_sleep;
		uiomux_sleep_timeout;
		uiomux_wakeup;
		uiomux_query;
		uiomux_name;
		uiomux_info;
		uiomux_meminfo;
		uiomux_get_mmio;
		uiomux_get_mem;
		uiomux_virt_to_phys;
		uiomux_all_virt_to_phys;
		uiomux_phys_to_virt;
		uiomux_malloc;
		uiomux_malloc_shared;
		uiomux_free;
		uiomux_register;
		uiomux_unregister;

		uiomux_dump_mmio;
		uiomux_dump_mmio_filename;

		uiomux_system_reset;
		uiomux_system_destroy;

        local:
                *;
};
