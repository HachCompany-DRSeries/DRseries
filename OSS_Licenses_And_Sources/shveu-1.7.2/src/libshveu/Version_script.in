#
# Export file for libshveu
#
# Only the symbols listed in the global section will be callable from
# applications linking to libuiomux.
#

@PACKAGE@.so.0.0
{
        global:
		shveu_open;
		shveu_close;
		shveu_setup;
		shveu_set_src;
		shveu_set_dst;
		shveu_set_src_phys;
		shveu_set_dst_phys;
		shveu_set_color_conversion;
		shveu_start;
		shveu_wait;
		shveu_start_locked;
		shveu_rescale;
		shveu_rotate;
		shveu_crop;

        local:
                *;
};
